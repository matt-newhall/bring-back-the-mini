{
  "name": "Bring Back the Mini",
  "description": "Small Chrome extension to make the NY Times Mini daily game playable again.",
  "version": "0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.nytimes.com/crosswords/game/mini"],
      "js": ["dist/enableContent.js"]
    }
  ]
}